                            android:textColor="@color/colorLoginforgetPassword"
                            android:textSize="@dimen/sp_11" />

                        <View
                            android:layout_width="1px"
                            android:layout_height="@dimen/dp35"
                            android:background="@color/colorgray" />

                        <TextView
                            android:id="@+id/tv_prize1_account"
