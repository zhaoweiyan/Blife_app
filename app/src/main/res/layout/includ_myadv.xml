                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="@string/bonus_money_one"
                            android:textColor="@color/colorLoginforgetPassword"
                            android:textSize="@dimen/sp_11" />
                    </LinearLayout>

                    <View
