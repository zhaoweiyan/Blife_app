                android:layout_height="1px"
                android:layout_marginLeft="@dimen/dp10"
                android:layout_marginRight="@dimen/dp10"
                android:background="@color/colorgray" />

            <TextView
                android:id="@+id/tv_description1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:lineSpacingExtra="@dimen/dp7"
                android:maxLines="4"
                android:paddingBottom="@dimen/dp20"
                android:paddingLeft="@dimen/dp14"
                android:paddingRight="@dimen/dp14"
                android:paddingTop="@dimen/dp20"
                android:text="@string/adv_detail_explain"
                android:textColor="@color/color_reset"
                android:textSize="@dimen/sp_13" />

            <TextView
                android:id="@+id/tv_description2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:lineSpacingExtra="@dimen/dp7"
                android:paddingBottom="@dimen/dp20"
                android:paddingLeft="@dimen/dp14"
                android:paddingRight="@dimen/dp14"
                android:paddingTop="@dimen/dp20"
                android:text="@string/adv_detail_explain"
                android:textColor="@color/color_reset"
                android:textSize="@dimen/sp_13"
                android:visibility="gone" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp10"
            android:background="@color/color_main_backround" />

        <LinearLayout
